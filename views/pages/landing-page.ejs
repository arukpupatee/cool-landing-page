          function search(id) {
              $.post('/search',{jobid:id})
              .done(function (data) {
                  //$location.path(data.results.name, false);
                  var url = window.location.href;
                  //window.location.href = url+"/";
                  var options = {duration:1000, animateOpacity: false, floatStepDecimals:0, floatEndDecimals:0};
                  if(id!=0) {
                      $("#c1").animateNumber(data.results.count, options);
                      if(!isNaN(data.results.salaryJ)) $("#c2J").animateNumber(data.results.salaryJ, options);
                      else $("#c2J").html(data.results.salaryJ);
                      if(!isNaN(data.results.salaryS)) $("#c2S").animateNumber(data.results.salaryS, options);
                      else $("#c2S").html(data.results.salaryS);
                      $("#c3").animateNumber(data.results.equity, options);
                      $("#c4").animateNumber(data.results.companies, options);
                      //var results = angular.element(document.getElementById('res'));
                      //$document.scrollToElement(results, 0, 1000);
                  }
              });
